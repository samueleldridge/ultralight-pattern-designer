# MVP Test Environment
# Copy this to .env and add your OpenAI API key

# OpenAI (REQUIRED)
OPENAI_API_KEY=your-key-here
OPENAI_MODEL=gpt-4-0125-preview

# Database (uses Docker defaults)
DATABASE_URL=postgresql+asyncpg://postgres:postgres@postgres:5432/aianalytics

# Redis (uses Docker defaults)
REDIS_URL=redis://redis:6379

# Clerk Auth (optional for MVP - can skip auth)
CLERK_SECRET_KEY=sk_test_placeholder
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_placeholder

# App
DEBUG=true
SECRET_KEY=mvp-secret-key-change-in-production
